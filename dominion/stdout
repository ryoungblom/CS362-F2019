gcc -c rngs.c -g  -Wall -fpic -coverage -lm -std=c99
gcc -c dominion.c -g  -Wall -fpic -coverage -lm -std=c99
gcc -o playdom playdom.c -g dominion.o rngs.o -Wall -fpic -coverage -lm -std=c99
gcc -c interface.c -g  -Wall -fpic -coverage -lm -std=c99
gcc -o player player.c -g  dominion.o rngs.o interface.o -Wall -fpic -coverage -lm -std=c99
gcc -o unittest1 dominion.c rngs.c unittest1.c -Wall -fpic -coverage -lm -std=c99
echo "unittest1.c:" >> unittestresults.out
./unittest1 >> unittestresults.out
gcc -o unittest2 dominion.c rngs.c unittest2.c -Wall -fpic -coverage -lm -std=c99
echo "unittest2.c:" >> unittestresults.out
./unittest2 >> unittestresults.out
gcc -o unittest3 dominion.c rngs.c unittest3.c -Wall -fpic -coverage -lm -std=c99
echo "unittest3.c:" >> unittestresults.out
./unittest3 >> unittestresults.out
gcc -o unittest4 dominion.c rngs.c unittest4.c -Wall -fpic -coverage -lm -std=c99
echo "unittest4.c:" >> unittestresults.out
./unittest4 >> unittestresults.out
gcc -o unittest5 dominion.c rngs.c unittest5.c -Wall -fpic -coverage -lm -std=c99
echo "unittest5.c:" >> unittestresults.out
./unittest5 >> unittestresults.out
gcov dominion.c -b -f >> unittestresults.out
